{
	"name": "docs",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"packageManager": "pnpm@10.11.0",
	"scripts": {
		"dev": "concurrently \"pnpm dev:content\" \"vite dev\"",
		"build": "pnpm build:content && pnpm build:svelte",
		"build:svelte": "vite build",
		"dev:content": "velite build --watch",
		"build:content": "velite",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint ."
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.9",
		"@eslint/js": "^9.27.0",
		"@fontsource-variable/jetbrains-mono": "^5.2.5",
		"@fontsource-variable/oxanium": "^5.2.5",
		"@lucide/svelte": "^0.511.0",
		"@lukulent/svelte-umami": "^0.0.4",
		"@pyncz/tailwind-mask-image": "^2.0.0",
		"@shikijs/langs": "^3.4.2",
		"@shikijs/markdown-it": "^3.4.2",
		"@shikijs/themes": "^3.4.2",
		"@sveltejs/adapter-vercel": "^5.7.2",
		"@sveltejs/kit": "^2.21.1",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@types/markdown-it": "^14.1.2",
		"@upstash/redis": "^1.34.9",
		"autoprefixer": "^10.4.20",
		"badge-maker": "^4.1.0",
		"bits-ui": "2.2.0",
		"clsx": "^2.1.1",
		"concurrently": "^9.1.2",
		"core@latest": "link:@shikijs/markdown-it/core@latest",
		"drizzle-orm": "^0.43.1",
		"eslint": "^9.27.0",
		"eslint-config-prettier": "^10.1.5",
		"eslint-plugin-svelte": "^3.9.0",
		"flexsearch": "^0.7.43",
		"globals": "^16.2.0",
		"isomorphic-dompurify": "^2.25.0",
		"jsrepo": "^2.2.0",
		"markdown-it": "^14.1.0",
		"markdown-it-table": "^4.1.1",
		"mdsx": "^0.0.7",
		"mode-watcher": "^0.5.1",
		"octokit": "^5.0.3",
		"package-manager-detector": "^1.3.0",
		"postgres": "^3.4.7",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.10",
		"rehype-autolink-headings": "^7.1.0",
		"rehype-external-links": "^3.0.0",
		"rehype-pretty-code": "^0.14.1",
		"rehype-raw": "^7.0.0",
		"rehype-slug": "^6.0.0",
		"rehype-stringify": "^10.0.1",
		"remark-gfm": "^4.0.1",
		"remark-parse": "^11.0.0",
		"remark-rehype": "^11.1.2",
		"remove-markdown": "^0.6.2",
		"runed": "^0.28.0",
		"shiki": "^3.4.2",
		"svelte": "^5.33.4",
